-- Exit_2.LocalScript is disabled.
local function XRLHLP_fake_script() -- Exit_2.Match 
	local script = Instance.new('LocalScript', Exit_2)

	while true do
		wait(0.01)
		script.Parent.BackgroundTransparency = script.Parent.Parent.BackgroundTransparency
		local Val = 0
		Val = script.Parent.Parent.BackgroundTransparency
		Val = Val+0.5
		script.Parent.TextTransparency = Val
	end
end
coroutine.wrap(XRLHLP_fake_script)()
