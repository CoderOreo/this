		script.Parent.Parent.Parent.Section.Credits.Visible = false
		script.Parent.Parent.Parent.Section["Executable Skins"].Visible = false
		script.Parent.Parent.Parent.Section.Backgrounds.Visible = false
		wait(0.01)
		script.Parent.Parent.Parent.Section[""..script.Parent.Name].Visible = true
	end)
end
