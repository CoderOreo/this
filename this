coroutine.wrap(LKNA_fake_script)()
local function MOZPUS_fake_script() -- TextLabel_6.Smooth GUI Dragging 
	local script = Instance.new('LocalScript', TextLabel_6)

	local UserInputService = game:GetService("UserInputService")
	local runService = (game:GetService("RunService"));
	
	local gui = script.Parent.Parent
	
	local dragging
	local dragInput
	local dragStart
	local startPos
	
	function Lerp(a, b, m)
		return a + (b - a) * m
	end;
	
	local lastMousePos
	local lastGoalPos
	local DRAG_SPEED = (8); -- // The speed of the UI darg.
	function Update(dt)
		if not (startPos) then return end;
		if not (dragging) and (lastGoalPos) then
			gui.Position = UDim2.new(startPos.X.Scale, Lerp(gui.Position.X.Offset, lastGoalPos.X.Offset, dt * DRAG_SPEED), startPos.Y.Scale, Lerp(gui.Position.Y.Offset, lastGoalPos.Y.Offset, dt * DRAG_SPEED))
			return 
		end;
	
		local delta = (lastMousePos - UserInputService:GetMouseLocation())
		local xGoal = (startPos.X.Offset - delta.X);
		local yGoal = (startPos.Y.Offset - delta.Y);
		lastGoalPos = UDim2.new(startPos.X.Scale, xGoal, startPos.Y.Scale, yGoal)
		gui.Position = UDim2.new(startPos.X.Scale, Lerp(gui.Position.X.Offset, xGoal, dt * DRAG_SPEED), startPos.Y.Scale, Lerp(gui.Position.Y.Offset, yGoal, dt * DRAG_SPEED))
	end;
	
	gui.InputBegan:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
			dragging = true
			dragStart = input.Position
			startPos = gui.Position
			lastMousePos = UserInputService:GetMouseLocation()
	
			input.Changed:Connect(function()
				if input.UserInputState == Enum.UserInputState.End then
					dragging = false
				end
			end)
		end
	end)
	
	gui.InputChanged:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
			dragInput = input
		end
	end)
	
	runService.Heartbeat:Connect(Update)
end
coroutine.wrap(MOZPUS_fake_script)()
local function XSAP_fake_script() -- Exit.LocalScript 
	local script = Instance.new('LocalScript', Exit)

	script.Parent.MouseButton1Click:Connect(function()
		local TweenService = game:GetService("TweenService")
		local PartTween = TweenService:Create(script.Parent.Parent, TweenInfo.new(), {BackgroundTransparency = 1})
		PartTween:Play()
		wait(1)
		script.Parent.Parent.Parent:Destroy()
		end)
end
coroutine.wrap(XSAP_fake_script)()
local function IGHXXI_fake_script() -- TextButton_2.LocalScript 
	local script = Instance.new('LocalScript', TextButton_2)

	script.Parent.MouseButton1Click:Connect(function()
		if script.Parent.Parent.Key.Text == "cookie1" then
			script.Parent.Parent.Visible = false
			script.Parent.Parent.Parent.MainFrame.Visible = true
			--[[
			Notifacation Area
			--]]
			local Title = "Welcome"
			local Desc = "Welcome to Chat Art Remake"
			local NewNotify = script.Parent.Parent.Parent.Notify:Clone()
			NewNotify.Parent = script.Parent.Parent.Parent.Notifications
			NewNotify.Title.Text = ""..Title
			NewNotify.Desc.Text = ""..Desc
			local TweenService = game:GetService("TweenService")
			local PartTween = TweenService:Create(NewNotify, TweenInfo.new(), {BackgroundTransparency = 0})
			PartTween:Play()
			local TweenService = game:GetService("TweenService")
			local PartTween = TweenService:Create(NewNotify.Exit, TweenInfo.new(), {BackgroundTransparency = 0})
			PartTween:Play()
			local TweenService = game:GetService("TweenService")
			local PartTween = TweenService:Create(NewNotify.Exit, TweenInfo.new(), {TextTransparency = 0.5})
			PartTween:Play()
			local TweenService = game:GetService("TweenService")
			local PartTween = TweenService:Create(NewNotify.Desc, TweenInfo.new(), {BackgroundTransparency = 0})
			PartTween:Play()
			local TweenService = game:GetService("TweenService")
			local PartTween = TweenService:Create(NewNotify.Desc, TweenInfo.new(), {TextTransparency = 0})
			PartTween:Play()
			local TweenService = game:GetService("TweenService")
			local PartTween = TweenService:Create(NewNotify.Title, TweenInfo.new(), {BackgroundTransparency = 0})
			PartTween:Play()
			local TweenService = game:GetService("TweenService")
			local PartTween = TweenService:Create(NewNotify.Title, TweenInfo.new(), {TextTransparency = 0})
			PartTween:Play()
			wait(4)
			local TweenService2 = game:GetService("TweenService")
			local PartTween2 = TweenService2:Create(NewNotify, TweenInfo.new(), {BackgroundTransparency = 1})
			PartTween2:Play()
			local TweenService = game:GetService("TweenService")
			local PartTween = TweenService:Create(NewNotify.Exit, TweenInfo.new(), {BackgroundTransparency = 1})
			PartTween:Play()
			local TweenService = game:GetService("TweenService")
			local PartTween = TweenService:Create(NewNotify.Exit, TweenInfo.new(), {TextTransparency = 1})
			PartTween:Play()
			local TweenService = game:GetService("TweenService")
			local PartTween = TweenService:Create(NewNotify.Desc, TweenInfo.new(), {BackgroundTransparency = 1})
			PartTween:Play()
			local TweenService = game:GetService("TweenService")
			local PartTween = TweenService:Create(NewNotify.Desc, TweenInfo.new(), {TextTransparency = 1})
			PartTween:Play()
			local TweenService = game:GetService("TweenService")
			local PartTween = TweenService:Create(NewNotify.Title, TweenInfo.new(), {BackgroundTransparency = 1})
			PartTween:Play()
			local TweenService = game:GetService("TweenService")
			local PartTween = TweenService:Create(NewNotify.Title, TweenInfo.new(), {TextTransparency = 1})
			PartTween:Play()
			wait(1)
			NewNotify:Destroy()
			--[[
			End of Notification Area
			--]]
		else
					--[[
			Notifacation Area
			--]]
			local Title = "Error"
			local Desc = "Incorrect key"
			local NewNotify = script.Parent.Parent.Parent.Notify:Clone()
			NewNotify.Parent = script.Parent.Parent.Parent.Notifications
			NewNotify.Title.Text = ""..Title
			NewNotify.Desc.Text = ""..Desc
			local TweenService = game:GetService("TweenService")
			local PartTween = TweenService:Create(NewNotify, TweenInfo.new(), {BackgroundTransparency = 0})
			PartTween:Play()
			local TweenService = game:GetService("TweenService")
			local PartTween = TweenService:Create(NewNotify.Exit, TweenInfo.new(), {BackgroundTransparency = 0})
			PartTween:Play()
			local TweenService = game:GetService("TweenService")
			local PartTween = TweenService:Create(NewNotify.Exit, TweenInfo.new(), {TextTransparency = 0.5})
			PartTween:Play()
			local TweenService = game:GetService("TweenService")
			local PartTween = TweenService:Create(NewNotify.Desc, TweenInfo.new(), {BackgroundTransparency = 0})
			PartTween:Play()
			local TweenService = game:GetService("TweenService")
			local PartTween = TweenService:Create(NewNotify.Desc, TweenInfo.new(), {TextTransparency = 0})
			PartTween:Play()
			local TweenService = game:GetService("TweenService")
			local PartTween = TweenService:Create(NewNotify.Title, TweenInfo.new(), {BackgroundTransparency = 0})
			PartTween:Play()
			local TweenService = game:GetService("TweenService")
			local PartTween = TweenService:Create(NewNotify.Title, TweenInfo.new(), {TextTransparency = 0})
			PartTween:Play()
			wait(2.4)
			local TweenService2 = game:GetService("TweenService")
			local PartTween2 = TweenService2:Create(NewNotify, TweenInfo.new(), {BackgroundTransparency = 1})
			PartTween2:Play()
			local TweenService = game:GetService("TweenService")
			local PartTween = TweenService:Create(NewNotify.Exit, TweenInfo.new(), {BackgroundTransparency = 1})
			PartTween:Play()
			local TweenService = game:GetService("TweenService")
			local PartTween = TweenService:Create(NewNotify.Exit, TweenInfo.new(), {TextTransparency = 1})
			PartTween:Play()
			local TweenService = game:GetService("TweenService")
			local PartTween = TweenService:Create(NewNotify.Desc, TweenInfo.new(), {BackgroundTransparency = 1})
			PartTween:Play()
			local TweenService = game:GetService("TweenService")
			local PartTween = TweenService:Create(NewNotify.Desc, TweenInfo.new(), {TextTransparency = 1})
			PartTween:Play()
			local TweenService = game:GetService("TweenService")
			local PartTween = TweenService:Create(NewNotify.Title, TweenInfo.new(), {BackgroundTransparency = 1})
			PartTween:Play()
			local TweenService = game:GetService("TweenService")
			local PartTween = TweenService:Create(NewNotify.Title, TweenInfo.new(), {TextTransparency = 1})
			PartTween:Play()
			wait(1)
			NewNotify:Destroy()
			--[[
			End of Notification Area
			--]]
		end
	end)
end
coroutine.wrap(IGHXXI_fake_script)()
local function KTPAOH_fake_script() -- ImageLabel_5.LocalScript 
	local script = Instance.new('LocalScript', ImageLabel_5)

	local frame = script.Parent
	
	
	
	local player = game.Players.LocalPlayer
	
	
	
	local userId = player.UserId
	
	local thumbType = Enum.ThumbnailType.AvatarBust
	local thumbSize = Enum.ThumbnailSize.Size420x420
	local content, isReady = game.Players:GetUserThumbnailAsync(userId, thumbType, thumbSize)
	frame.Image = content
end
coroutine.wrap(KTPAOH_fake_script)()
